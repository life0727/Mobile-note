      html{
        font-size: 62.5% !important;
        /* font-family:"Arial","Microsoft YaHei","黑体",sans-serif!important; */
        }
       body{
        background: rgba(230,230,230,.5)
       }
       .SearchBegin{
        cursor: pointer;
        padding: 3px 18px;
        font-size: 14px;
        background-color: #00b38a;
        border-color:#00b38a;
        color: white;
        line-height: 45px;
        margin-left: 30px;
        border-radius: 4px;
       }
       .el-loading-spinner i{
        color:#00b38a !important;
       }
       .el-loading-spinner .el-loading-text{
        color:#00b38a !important;
       }
       .page>button:hover i{
        color:#00b38a !important;
       }
       .page .el-pager li:hover{
        color:#00b38a !important;
       }
       .el-message{
        background-color: #090723 !important;
        border-color: #090723 !important;
        border-radius: 0 !important;
        top:0 !important;
        width: 100%;
        height: 158px;
       }
       .el-message .el-icon-warning {
        display: none !important;
       }
       .el-message .el-icon-success {
        display: none !important;
       }
       .el-message .el-message__closeBtn{
        top: 25px;
        color:white;
       }
       .el-message .el-message__content{
        color: white !important;
        margin: 0 auto !important;
        font-size: 20px !important;
       }
       button:hover{
        opacity: 0.8;
        }
       #search:hover{
        opacity: 0.8;
        }  
